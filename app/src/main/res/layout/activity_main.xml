<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.project.ken.botec.MainActivity"
    android:paddingTop="20dp">


    <FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:id="@+id/frameLayout3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="32dp">

        <at.markushi.ui.CircleButton
            android:id="@+id/stock_in_button"
            android:layout_width="150dp"
            android:layout_height="200dp"
            android:src="@drawable/ic_out_white"
            app:cb_color="#99CC00"
            app:cb_pressedRingWidth="8dip" />
    </FrameLayout>

    <FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:id="@+id/frameLayout"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/frameLayout3"
        android:layout_alignStart="@+id/frameLayout3"
        android:layout_below="@+id/frameLayout3"
        android:layout_marginTop="27dp">

        <at.markushi.ui.CircleButton
            android:id="@+id/stock_out_button"
            android:layout_width="150dp"
            android:layout_height="200dp"
            android:src="@drawable/ic_in_white"
            app:cb_color="#D50000"
            app:cb_pressedRingWidth="8dip" />
    </FrameLayout>





</RelativeLayout>
